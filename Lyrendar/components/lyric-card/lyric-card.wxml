<view class="lyric-card">
  <image class="album-cover" src="{{albumCover}}" mode="aspectFill"/>
  <view class="lyric-content">
    <text class="song-title">{{songTitle}}</text>
    <text class="artist">{{artist}}</text>
    <text class="lyric">{{lyric}}</text>
  </view>
  <text class="date">{{date}}</text>
</view>